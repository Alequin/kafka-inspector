{
  "name": "k-inspect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "print-schema": "node scripts/print-schema",
    "start": "node src/server/server.js production",
    "start:dev": "node src/server/server.js",
    "build": "rm -rf build && node scripts/build.js",
    "build:dev": "rm -rf dist && npx webpack --config webpack/webpack.config.js",
    "test": "node scripts/run-tests.js",
    "migrate:create": "db-migrate create --sql-file --config ./_server-common/database/database.json",
    "migrate:up": "db-migrate up --config ./_server-common/database/database.json -e ",
    "migrate:down": "db-migrate down --config ./_server-common/database/database.json -e "
  },
  "devDependencies": {
    "babel-jest": "24.1.0",
    "jest": "24.1.0",
    "jest-pnp-resolver": "1.2.0",
    "jest-resolve": "24.1.0",
    "jest-watch-typeahead": "^0.2.1",
    "mock-test-data": "file:_mock-test-data",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "4.1.0",
    "apollo-boost": "^0.3.1",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.1",
    "bfj": "6.1.1",
    "cache": "^2.1.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "1.0.0",
    "db-migrate": "^0.11.5",
    "db-migrate-sqlite3": "^0.4.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "express": "^4.16.4",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.1",
    "graphql": "^14.2.1",
    "graphql-tools": "^4.0.4",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "kafka-node": "^4.0.2",
    "kafkajs": "^1.4.8",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.3",
    "react-apollo": "^2.5.3",
    "react-app-polyfill": "^0.2.1",
    "react-dev-utils": "^7.0.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "server-common": "file:_server-common",
    "sqlite3": "^4.0.6",
    "style-loader": "0.23.1",
    "styled-components": "^4.2.0",
    "tcp-ping": "^0.1.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "uuid": "^3.3.2",
    "webpack": "4.28.3",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/_server-common/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/_server-common/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/home/developer/work/k-inspect/node_modules/jest-watch-typeahead/filename.js",
      "/home/developer/work/k-inspect/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
